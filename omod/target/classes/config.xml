<?xml version="1.0" encoding="UTF-8"?>
<module configVersion="1.2">
    <id>pinnaclecustomemrmodule</id>
    <name>Pinnacle Custom EMR</name>
    <version>1.0.0-SNAPSHOT</version>
    <package>org.openmrs.module.pinnaclecustomemrmodule</package>

    <mappingFiles>moduleApplicationContext.xml</mappingFiles>

    <!-- THIS SERVLET MAPPING WORKS WITH YOUR CURRENT CODE -->
    <servlet>
        <servlet-name>pinnacleRest</servlet-name>
        <servlet-class>org.springframework.web.servlet.DispatcherServlet</servlet-class>
        <init-param>
            <param-name>contextConfigLocation</param-name>
            <param-value>/WEB-INF/webModuleApplicationContext.xml</param-value>
        </init-param>
        <load-on-startup>1</load-on-startup>
    </servlet>

    <servlet-mapping>
        <servlet-name>pinnacleRest</servlet-name>
        <url-pattern>/pinnacle/api/v1/*</url-pattern>
    </servlet-mapping>

</module>